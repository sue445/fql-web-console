<table class="table table-striped table-bordered table-hover">
  <% if @res.length > 0 %>
      <thead>
      <tr>
        <% @res[0].each do |key, value| %>
            <th><%= key %></th>
        <% end %>
      </tr>
      </thead>
      <tbody>
        <% @res.each do |row| %>
            <tr>
              <% row.each do |key, value| %>
                <td><%= parse_link value %></td>
              <% end %>
            </tr>
        <% end %>
      </tbody>
  <% end %>

</table>
